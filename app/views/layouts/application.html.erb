<!-- // -->
  </head>
  <body>
    <%= render 'shared/navbar' %>
<!--     <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p> -->
    <%= yield %>
    <%= render 'shared/footer' %>
    <script src="https://cdn.jsdelivr.net/instantsearch.js/1/instantsearch.min.js"></script>

    <%= javascript_include_tag 'application' %>
    <%= yield(:after_js) %>
  </body>
</html>
